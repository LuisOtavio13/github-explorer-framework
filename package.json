{
  "name": "github-api-framework",
  "version": "1.0.0",
  "description": "A modular framework for interacting with the GitHub API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c --configPlugin terser",
    "build:dev": "rollup -c --configPlugin terser --environment NODE_ENV:development",
    "build:prod": "rollup -c --configPlugin terser --environment NODE_ENV:production",
    "build:watch": "rollup -c --watch --configPlugin terser",
    "build:analyze": "rollup -c --configPlugin terser --sourcemap"
  },
  "keywords": [
    "github",
    "api",
    "framework",
    "wrapper"
  ],
  "author": "TylorSwift2",
  "license": "GPL-3.0-only",
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "rollup": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/github-api-helper.esm.js",
      "require": "./dist/github-api-helper.cjs.js",
      "default": "./dist/github-api-helper.min.js"
    },
    "./package.json": "./package.json"
  }
}
